{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off",
        "image-alt": "off"
      }
    ],
    "axe/parsing": [
      "default",
      {
        "duplicate-id": "off",
        "duplicate-id-active": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off",
        "button-name": "off"
      }
    ],
    "axe/structure": [
      "default",
      {
        "list": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "user-select",
          "height: fit-content",
          "width: fit-content",
          "-webkit-text-size-adjust"
        ]
      }
    ],
    "button-type": "off",
    "disown-opener": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not ios_saf <= 14.8"
  ]
}